<?xml version="1.0" encoding="utf-8"?>
<resources>

    <declare-styleable name="RegularEditText">
        <attr name="ret_input_type" format="enum">
            <enum name="email_or_phone" value="0" />
            <enum name="password" value="1" />
            <enum name="phone_number" value="2" />
            <enum name="text" value="3"/>
        </attr>
        <attr name="ret_hint" format="string" />
        <attr name="ret_floating_hint" format="string" />
        <attr name="ret_password_validation_enabled" format="boolean" />
    </declare-styleable>

</resources>